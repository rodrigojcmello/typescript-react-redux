{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-single"
        ],
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "no-console": false
    },
    "rulesDirectory": []
}